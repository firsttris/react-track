  "main": "main.js",
  "scripts": {
    "start:web": "webpack-dev-server --env.config=dev --env.app=web --color",
    "start:electron": "concurrently \"yarn webpack:electron\" \"wait-on http://localhost:8081 && electron . --dev\"",
    "start:both": "concurrently \"yarn start:electron\" \"yarn start:web\"",
    "webpack:electron": "webpack-dev-server --port 8081 --env.config=dev --env.app=electron --color",
    "build:electron": "webpack --env.config=prod --env.app=electron",
    "build:web": "webpack --env.config=prod --env.app=web",
    "package:electron": "yarn build:electron && electron-packager ./ --out=./builds --overwrite",
    "start:electron:prod": "electron ."
  },
  "devDependencies": {
