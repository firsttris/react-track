  "homepage": "https://teufel-it.de",
  "main": "main.js",
  "scripts": {
    "start:web": "webpack-dev-server --env.config=dev --env.app=web --color",
    "start:electron": "concurrently \"yarn webpack:electron\" \"wait-on http://localhost:8081 && electron . --dev\"",
    "start:both": "concurrently \"yarn start:electron\" \"yarn start:web\"",
    "webpack:electron": "webpack-dev-server --port 8081 --env.config=dev --env.app=electron --color",
    "build:electron": "webpack --env.config=prod --env.app=electron",
    "build:web": "webpack --env.config=prod --env.app=web",
    "package:electron": "yarn build:electron && electron-packager ./ --out=./builds --overwrite",
    "start:electron:prod": "electron ."
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/lodash": "4.14.137",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/reactstrap": "^8.0.1",
    "@types/recharts": "^1.1.20",
    "@types/enzyme": "^3.10.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "electron": "^6.0.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "file-loader": "^4.2.0",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-day-picker": "^7.3.2",
    "react-dom": "^16.9.0",
    "react-intl": "^3.1.9",
    "react-hot-loader": "^4.12.11",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "16.9.0",
    "react-transition-group": "^4.2.2",
    "reactstrap": "^8.0.1",
