{
  "private": true,
  "name": "react-track",
  "version": "1.0.0",
  "description": "timetracking app",
  "author": "Tristan Teufel",
  "license": "UNLICENSED",
  "homepage": "https://teufel-it.de",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.17",
    "@types/lowdb": "1.0.9",
    "@types/moment-duration-format": "2.2.2",
    "@types/uuid": "^3.4.5",
    "@types/prop-types": "^15.7.1",
    "@types/fs-extra": "^8.0.0",
    "ts-loader": "^6.0.4",
    "clean-webpack-plugin": "^3.0.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.4.2",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-typescript-template": "^0.18.2",
    "jest": "^24.9.0",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6"
  },
  "repository": {
    "type": "git",
    "url": "https://tristanteu@bitbucket.org/teufelit/timetracking-app.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:web+server": "concurrently \"yarn start:server\" \"yarn start:web\"",
    "start:web": "cd packages/app && yarn start:web",
    "start:server": "cd packages/server && yarn start:server",
    "test": "jest",
    "tslint:fix": "tslint --fix -c tslint.json \"packages/**/*.{ts,tsx}\"",
    "generate-types": "gql-gen --schema http://localhost:3001/graphql --template graphql-codegen-typescript-template --out packages/common/gen.ts \"packages/app/src/graphql/*.ts\"",
    "build:web": "cd packages/app && yarn build:web",
    "build:server": "cd packages/server && yarn build"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
